# Function to process each ETRECORD_FR entry
plot_heatmap_from_list <- function(coord_list, row_id = NULL) {
# Skip if it's all NAs or empty
if (all(is.na(coord_list)) || length(coord_list) == 0) return(NULL)
# Try to extract x and y from each sublist
coords <- coord_list %>%
keep(~ is.list(.x) && length(.x) >= 2) %>%
map_df(~{
x <- as.numeric(.x[[1]])
y <- as.numeric(.x[[2]])
tibble(x = x, y = y)
}) %>%
filter(!is.na(x) & !is.na(y))
# Skip if no valid points
if (nrow(coords) == 0) return(NULL)
# Plot with ggplot
p <- ggplot(coords, aes(x = x, y = y)) +
stat_density_2d(aes(fill = ..level..), geom = "polygon", contour = TRUE) +
scale_fill_viridis_c(option = "plasma") +
coord_fixed(ratio = 9/16, xlim = c(-1, 1), ylim = c(-1, 1), expand = FALSE) +
theme_minimal() +
labs(title = paste("Heatmap - Row", row_id))
return(p)
}
